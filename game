 Take input from the user to create the file. '''
def isfloat(n):
 try:
 float(n)
 return True
 except ValueError:
 return False

print('Enter any type of data into your file : ')
print('Press \'END\' when you are done. ')
f = open("test.txt",'w')
fi = open("integer.txt",'w')
fd = open("decimal.txt",'w')
fs = open("string.txt",'w')
while True:
 line=input()
 if line in ('END','end','End'):
 break;
 f.write(line)
 f.write('\n')
f.close()
f=open("test.txt",'r')
for i in f:

 if i.isdecimal():
 fi.write(i)
 elif isfloat(i):
 fd.write(i)
 else:
 fs.write(i)

f.close()
fi.close()
fd.close()
fs.close()
print('\nYour File Data : \n')
f=open("test.txt",'r')
print(f.read())
f.close()
print('\nInteger File Data : \n')
fi = open("integer.txt",'r')
print(fi.read())
fi.close()
print('\nFloat File Data : \n')
fd = open("decimal.txt",'r')
print(fd.read())
fd.close()
print('\nString File Data : \n')
fs = open("string.txt",'r')
print(fs.read())
fs.close()
