are non-zero. '''
print('Enter Numbers into your file : ')
print('Press zero(0) when you are done. ')
f = open("test.txt",'w')
fo = open("odd.txt",'w')
fe = open("even.txt",'w')
while True:
 line=input()
 if line == '0':
 break;
 f.write(line)
 f.write('\n')
f.close()
f=open("test.txt",'r')
for i in f:
 no = int(i)
 if no%2 == 0:
 fe.write(i)
 else:
 fo.write(i)
f.close()
fe.close()
fo.close()
print('\nYour File Data : \n')
f=open("test.txt",'r')
print(f.read())
f.close()
print('\nEven File Data : \n')
fe = open("even.txt",'r')
print(fe.read())
fe.close()
print('\nOdd File Data : \n')
fo = open("odd.txt",'r')
print(fo.read())
fo.close()
