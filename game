     #stack###
     class Stack:
    stackList = []
    top = -1

    def __init__(self):
        pass
        #self.size = size

    def push(self,val):
        self.top += 1
        self.stackList.append(val)
    
    def pop(self):
        if(self.isUnderFlow()):
            print("Stack is UnderFlow")
            return
        
        print('Pop val : ',self.stackList[self.top])
        del self.stackList[self.top]
        self.top -= 1
        
    def peep(self):
        if(self.isUnderFlow()):
            print("Stack is UnderFlow")
            return
        print('Peep value : ',self.stackList[self.top])

    def isUnderFlow(self):
        if(self.top == -1):
            return True
        else:
            return False
        
    def display(self):
        if(self.isUnderFlow()):
            print("Stack is UnderFlow")
            return
        
        print("Stack Elements")
        for i in self.stackList:
            print(i)
            
    def __del__(self):
        print("stack deleted")

s1 = Stack()
while(True):
    print("-"*10)
    print("1) Push")
    print("2) Pop")
    print("3) Peep")
    print("4) Display")
    print("5) Exit")
    print("-"*10)
    ch = input("Enter Your Choice : ")

    if ch == '1':
        val = input("Enter Value : ")
        s1.push(val)
    elif ch == '2':
        s1.pop()
    elif ch == '3':
        s1.peep()
    elif ch == '4':
        s1.display()
    elif ch == '5':
        print("Exit_x_0")
        break
    else:
        print("Wrong Input !!!")

del s1





