oglist = []
length = 0

while(True):
    val = input("Enter Number of 3-digit (0 to exit): ")
    if(val == '0'):
        break
    c = 0
        
    try:
        val = int(val)
        for i in str(val):
            c = c + 1
        if(c == 3):
            oglist.append(val)
            length += 1
        else:
            print("val not in 3 digits")

    except:
        print("Not a  Number")

#reverse list
revlist = []
for i in range(length-1,-1,-1):
    revlist.append(oglist[i])

#reverse Numberse in revList

mainlist = []
for val in revlist:
    og = val
    rem = 0
    rev = 0

    while(og != 0):
        rem = og%10
        rev = rev*10 + rem
        og = int(og/10)
        
    mainlist.append(rev)

print("og list : ",oglist)
print("revlist : ",revlist)
print("revNum revList : ",mainlist)



